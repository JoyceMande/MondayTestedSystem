
@{
    ViewBag.Title = "Import Customer details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <centre>
        @using (Html.BeginForm("UploadExcel", "Import", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
        {
        <div class="form-horizontal">

            <div class="form-group">

                <div class="control-label col-md-2">Excel:</div>
                <div class="col-md-10">
                    <h5 style="color:red">@ViewBag.Error</h5>
                    <h5 style="color:green">@ViewBag.Success</h5>
                    <input type="file" id="FileUpload" name="FileUpload" class="" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Upload" id="btnSubmit" class="btn btn-primary" />

                </div>
            </div>
            <div>
                @Html.ActionLink("Back to List", "List", "Customers")
            </div>
        </div>
        }
    </centre>